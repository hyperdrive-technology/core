{
  "name": "@inrush/ide",
  "private": true,
  "version": "0.1.0",
  "main": "src-gen/frontend/index.js",
  "typings": "src-gen/frontend/index.d.ts",
  "files": [
    "lib",
    "src-gen"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn build",
    "clean": "rimraf lib .browser_modules",
    "build": "yarn build:backend && yarn build:frontend",
    "build:backend": "tsc -p backend/tsconfig.json",
    "build:frontend": "yarn rebuild:browser && webpack --mode=production",
    "rebuild:browser": "theia rebuild:browser",
    "start": "node ./lib/backend/main.js",
    "start:debug": "node --inspect ./lib/backend/main.js",
    "watch": "concurrently -n backend,frontend \"yarn watch:backend\" \"yarn watch:frontend\"",
    "watch:backend": "tsc -w -p backend/tsconfig.json",
    "watch:frontend": "webpack --mode=development --watch"
  },
  "dependencies": {
    "@theia/core": "^1.47.0",
    "@theia/editor": "^1.47.0",
    "@theia/filesystem": "^1.47.0",
    "@theia/languages": "^1.47.0",
    "@theia/markers": "^1.47.0",
    "@theia/messages": "^1.47.0",
    "@theia/monaco": "^1.47.0",
    "@theia/navigator": "^1.47.0",
    "@theia/preferences": "^1.47.0",
    "@theia/process": "^1.47.0",
    "@theia/terminal": "^1.47.0",
    "@theia/workspace": "^1.47.0"
  },
  "devDependencies": {
    "@theia/cli": "^1.47.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/frontend/ide-frontend-module",
      "backend": "lib/backend/ide-backend-module"
    }
  ]
}
